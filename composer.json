{
    "name": "dpc/guzzle-client",
    "description": "Simple Guzzle Client for a Laravel application",
    "type": "libr",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "5.4.*||5.5.*",
        "phpunit/phpunit": "^6.3",
        "orchestra/database": "^3.5",
        "orchestra/testbench": "^3.5"
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Dpc\\GuzzleClient\\": "src",
            "Dpc\\Tests\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dpc\\GuzzleClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dpc\\GuzzleClient\\GuzzleClientServiceProvider"
            ]
        }
    }
}
